           MOVE    PTBYOMEI-REC        TO  MCPDATA-REC
           MOVE    "DBSELECT"          TO  MCP-FUNC
           MOVE    "PTBYOMEI-KEY2"     TO  ORC-DBPATH
           CALL    "ORCMCPSUB"          USING
                                        MCPAREA
                                        ORCMCPAREA
                                        MCPDATA-REC
           IF      MCP-RC              =   ZERO
               MOVE    "PTBYOMEI-KEY2"     TO  ORC-DBPATH
               PERFORM 950-PTBYOMEI-READ-SEC
           ELSE
               MOVE    1                   TO  FLG-PTBYOMEI
           END-IF
           PERFORM 
                   UNTIL   FLG-PTBYOMEI    NOT =   ZERO
               ADD     1               TO  IDX
               IF      WRK-PRT-CNT     =   1
                   IF      IDX             >   10
      *                カルテ印刷
                       PERFORM 2003-HCN01-PRINT-SEC
      *                続紙初期編集
                       PERFORM 2004-HCN011-HENSYU-SEC
                       ADD     1               TO  WRK-PRT-CNT
                       MOVE    1               TO  IDX
                   END-IF
               ELSE
      *            続紙印刷
                   IF      IDX             >   22
      *                続紙印刷
                       PERFORM 2005-HCN011-PRINT-SEC
      *                続紙初期編集
                       PERFORM 2004-HCN011-HENSYU-SEC
                       ADD     1               TO  WRK-PRT-CNT
                       MOVE    1               TO  IDX
                   END-IF
               END-IF
      *
               IF      WRK-PRT-CNT         =   1
      *            カルテ病名編集処理
                   PERFORM 3103-HCN01-BYOMEI-SET-SEC
               ELSE
      *            続紙病名編集処理
                   PERFORM 3104-HCN011-BYOMEI-SET-SEC
               END-IF
      *
               MOVE    "PTBYOMEI-KEY2"     TO  ORC-DBPATH
               PERFORM 950-PTBYOMEI-READ-SEC
           END-PERFORM
      *
           IF      IDX             >   ZERO
               IF      WRK-PRT-CNT     =   1
      *            カルテ印刷
                   PERFORM 2003-HCN01-PRINT-SEC
               ELSE
      *            続紙印刷
                   PERFORM 2005-HCN011-PRINT-SEC
               END-IF
           END-IF
