#!/bin/sh
if test -n "$JMARECEIPT_ENV"; then
    JMARECEIPT_ENV="$JMARECEIPT_ENV"
elif test -n "@jma-receipt-env@" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi

if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV
#----------------------------------------------------------------------#
#       引数	
#       1:DATAファイル名称
#----------------------------------------------------------------------#

	echo	`date`
	echo	$1

ruby "$SCRIPTSDIR"/daily/print_parent2.rb "/var/tmp/temp.tmp" "ruby ${SCRIPTSDIR}/daily/print-data.rb " $1 

	rm -f $1
	echo	`date`
