#!/bin/sh

if test -n "@jma-receipt-env@" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

echo	`date`
echo	$1
echo	$2
echo	$3
echo	$4
echo	$5

ruby $SCRIPTSDIR/daily/print_parent.rb "temp.tmp" "ruby ${SCRIPTSDIR}/print-data.rb " $1 $2 $3 $4 $5

echo	`date`
