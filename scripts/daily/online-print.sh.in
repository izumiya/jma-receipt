#!/bin/bash

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

#-------------------------------------------#
#    オンライン帳票印刷処理
#        $1  PGID
#        $2  PARA01
#        $3  PARA02
#        $4  PARA03
#        $5  PARA04
#        $6  PARA05
#        $7  FLG(CALL形式  (共通:1/独自:BLANK)
#-------------------------------------------#
LOGFILE=/tmp/online-print.log

	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCBHCPRT -parameter $1,$2,$3,$4,$5,$6,$7  2>&1 | tail -n 1000 > ${LOGFILE} 

	[ -f "$2" ] && rm $2
	[ -f "$3" ] && rm $3
	[ -f "$4" ] && rm $4
	[ -f "$5" ] && rm $5
	[ -f "$6" ] && rm $6
        
	exit

