#!/bin/sh

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

#-------------------------------------------#
#    レセ電ファイル作成処理
#        $1-${11} 印刷ＤＢ用固定引数
#        $12 請求年月
#        $13 提出先
#        $14 レセ電ファイル出力先
#        $15 JOBID 
#        $16 SHELLID
#        $17 エラーファイル名 
#-------------------------------------------#
##      エラーファイル削除
	echo $#
	echo "echo " ${17}

        $DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCBM500 -parameter $1,$2,$3,$RENNUM,$5,$6,$7,$8,$9,${10},${11},${12},${13},${15},${16}
        if  [ -e ${17} ]; then
            rm  ${17}
        fi
        
        $DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCBM510 -parameter $1,$2,$3,$RENNUM,$5,$6,$7,$8,$9,${10},${11},${12},${13},${15},${16}
        
        
        if  [ -e ${17} ]; then
            exit 
        fi
        
        $DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCBJOB -parameter JBE${15}${16}

        

#       漢字コード変換（ＥＵＣからシフトＪＩＳ）	
        nkf -s /var/tmp/RECE300.dat > ${14}RECEIPTC.UKE

        exit 
