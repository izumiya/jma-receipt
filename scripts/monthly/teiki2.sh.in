#!/bin/sh

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

#-------------------------------------------#
#    定期請求データ作成
#
#        $1  PRTJYUN      -- 出力順
#        $2  BTUNUM-MAX   -- 処理対象病棟件数
#        $3  BTUNUM       -- 処理対象病棟（最大５０）
#        $4  システム日付
#        $5  TERMID
#        $6  JOBID 
#        $7  SHELLID
#        $8  HOSPID
#        $9  STAFFCD
#        $10 エラーファイル
#-------------------------------------------#
##      エラーファイル削除
	echo $#
	echo "echo " ${10}
        if  [ -e ${10} ]; then
            rm  ${10}
        fi
        
        rm  /var/tmp/TEIKI*

##   -- ORCT0020 START --  
	$DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCT0020 -parameter $1,$2,$3,$4,$5,$6,$7,$8,$9,
        if  [ -e ${10} ]; then
            exit  
        fi 

	$DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCBJOB -parameter JBE$6$7
	
        exit

