#! /bin/sh

if test -z "$JMARECEIPT_ENV"; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

echo DATADIR = $DATADIR
echo DOCDIR = $DOCDIR
echo FORMDIR = $FORMDIR
echo INITDIR = $INITDIR
echo LDDEFDIR = $LDDEFDIR
echo RECORDDIR = $RECORDDIR
echo SCREENDIR = $SCREENDIR
echo SCRIPTSDIR = $SCRIPTSDIR
echo ORCALIBDIR  = $ORCALIBDIR
echo SITEDIR = $SITEDIR
echo SITESRCDIR = $SITESRCDIR
echo SITELIBDIR = $SITELIBDIR

echo SITERECORDDIR = $SITERECORDDIR
echo SITELDDEFDIR = $SITELDDEFDIR

echo DBSTUB = $DBSTUB

echo DataBaseName = $DBNAME

# example
echo $DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCBQ01 -parameter

# CHIHOUKOUHI example
echo $DBSTUB -dir "$LDDEFDIR"/directory -bd shimane ORCBS321 -parameter $1

exit 0
