    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

#------------------------------------
# $1 : 医療機関識別番号(HOSPNUM)
# $2 : スーパーバイザー
#------------------------------------

#標準マスタ更新
sh $SCRIPTSDIR/allways/master_standard_upgrade.sh $1 $2

#ライセンスマスタ更新
sh $SCRIPTSDIR/allways/master_license_upgrade.sh $1 $2
