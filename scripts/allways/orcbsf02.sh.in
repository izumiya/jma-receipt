#!/bin/sh

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV
#-------------------------------------------#
#    バックアップ処理
#        $1   TABLE-NAME
#        $2   CNV-NAME
#        $3   OUT-NAME-WK
#        $4$5 OUT-NAME
#        $6   KBEN
#        $7   HOSPNUM
#-------------------------------------------#
##

        if  [ -e $4$5 ]; then
            rm  $4$5
        fi

        pg_dump -R $DBNAME > $4$5

	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCBJOB -parameter JBE0000001ORCBSF1,${7}

        exit