#!/bin/sh

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV
#-------------------------------------------#
#    自院病名一覧出力設定
#        $1 TERMID     X(32)
#        $2 SYSYMD     X(8)
#        $3 HOSPID     X(24)
#        $4 SYUKBN     X(1)
#-------------------------------------------#
    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORHCML09 -parameter $1,$2,$3,$4
