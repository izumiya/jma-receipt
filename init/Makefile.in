INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_SCRIPT = @INSTALL_SCRIPT@
INSTALL_DATA = @INSTALL_DATA@


prefix = @prefix@
exec_prefix= @exec_prefix@
initdir = ${exec_prefix}/init

init_SCRIPT = \
        orca-db-create.sh \
        orca-db-install.sh

EXTRA_DIST = \
             orca_dbkanri_orig.dump \
             orca_user.dump

all:   
	touch build-stamp
clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f Makefile

install: all
	$(INSTALL) -d $(initdir)
	$(INSTALL_SCRIPT) $(init_SCRIPT) $(initdir)
	$(INSTALL_DATA) $(EXTRA_DIST) $(initdir)

