INSTALL = @INSTALL@
CP = cp

prefix = @prefix@
exec_prefix= @exec_prefix@
sysconfdir = @sysconfdir@
install_ignore := Makefile.in Makefile build-stamp

all:   
	touch build-stamp
clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f Makefile

install: all
	$(INSTALL) -d $(etcdir)
	$(CP) -R * $(etcdir)/; \
	for i in $(install_ignore) ;do rm -f $(etcdir)/$$i; done
