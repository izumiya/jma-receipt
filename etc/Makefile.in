INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@

prefix = @prefix@
exec_prefix= @exec_prefix@
sysconfdir = @sysconfdir@

EXTRA_DIST = dbgroup.inc kentan.inc jma-receipt.env

all:   
	touch build-stamp

filecheck:

clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f jma-receipt.env
	-rm -f Makefile

install: all
	$(INSTALL) -d $(sysconfdir)
	for f in $(EXTRA_DIST); do \
	  $(INSTALL_DATA) $${f} $(sysconfdir)/$${f}-dist ;\
	  if [ ! -f $(sysconfdir)/$${f} ]; then \
		$(INSTALL_DATA) $${f} $(sysconfdir)/$${f};  \
	  fi ; \
	done 
