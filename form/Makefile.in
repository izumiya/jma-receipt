COBOL = cobc
INSTALL = @INSTALL@
MKDIR = mkdir -p
CP = cp

prefix = @prefix@
exec_prefix= @exec_prefix@
orcadir = ${exec_prefix}/orca
formdir = ${orcadir}/form
install_ignore := Makefile.in Makefile build-stamp

all:   
	touch build-stamp
clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f Makefile

install: all
	$(INSTALL) -d $(formdir)
	$(CP) * $(formdir)/; \
	for i in $(install_ignore) ;do rm -f $(formdir)/$$i; done
