		FROM tbl_santei
			WHERE   HOSPID    =   :HOSPID
			AND   PTID        =   :PTID
			AND   SRYYM       =   :SRYYM
			ORDER   BY      SRYCD,
                                        NYUGAIKBN,
                                        SRYKA,
                                        HKNCOMBINUM
		;
	};
};

