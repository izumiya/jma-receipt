primary	{
	HOSPID,PTID,SRYYM,SRYCD,NYUGAIKBN,SRYKA,HKNCOMBINUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_santei_key_csr CURSOR FOR
		SELECT *
		FROM tbl_santei
			WHERE   HOSPID    =   :HOSPID
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   SRYKA       =   :SRYKA
			AND   SRYYM       =   :SRYYM
			AND   SRYCD       =   :SRYCD
			AND   HKNCOMBINUM =   :HKNCOMBINUM
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_santei_key2_csr CURSOR FOR
