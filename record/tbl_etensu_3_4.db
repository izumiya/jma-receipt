tbl_etensu_3_4 {
	SRYCD1				char(9);
	SRYCD2				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	HAIHAN				number(1,0);
	TOKUREI				number(1,0);
	CREYMD   			char(8);
};
primary	{
	SRYCD1,SRYCD2,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_3_4_key_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_3_4
		WHERE   SRYCD1   =   :SRYCD1
		AND     SRYCD2   =   :SRYCD2
 		AND   YUKOSTYMD  <=  :YUKOSTYMD
		AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_etensu_3_4_all_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_3_4
			ORDER   BY  SRYCD1,
				    SRYCD2,
				    YUKOSTYMD
 		;
	};
};
