tbl_example {
	HOSPID				char(24);
	KUBUN				char(1);
	RENNUM       		number(3);
	COMMENT        		varchar(800);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD    			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPID,KUBUN,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_example_key_csr CURSOR FOR
		SELECT *
		FROM tbl_example
			WHERE   HOSPID    =   :HOSPID
			AND   KUBUN       =   :KUBUN
			AND   RENNUM      =   :RENNUM
		;
	};
};


path	key1		{ 	
	DBSELECT	{
		DECLARE tbl_example_key1_csr CURSOR FOR
		SELECT *
		FROM tbl_example
			WHERE   HOSPID    =   :HOSPID
			AND   KUBUN       =   :KUBUN
			ORDER   BY        RENNUM
		;
	};
};

path	all	{ 	
	DBSELECT	{
		DECLARE tbl_example_all_csr CURSOR FOR
		SELECT *
		FROM tbl_example
 		;
	};
};
path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_example_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_example
			WHERE   HOSPID    =   :HOSPID
			AND   KUBUN       =   :KUBUN
			ORDER   BY        RENNUM  DESC
		;
	};
};

