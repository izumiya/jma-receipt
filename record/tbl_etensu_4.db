tbl_etensu_4 {
	N_GROUP				number(3,0);
	SRYCD				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	KASAN				number(1,0);
	CHGYMD   			char(8);
};
primary	{
	N_GROUP,SRYCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_4_key_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_4
		WHERE   N_GROUP  =   :N_GROUP
		AND     SRYCD    =   :SRYCD
 		AND   YUKOSTYMD  <=  :YUKOSTYMD
		AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_4_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_4
		WHERE   N_GROUP  =   :N_GROUP
			ORDER   BY  SRYCD,
				    YUKOSTYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_etensu_4_all_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_4
			ORDER   BY  N_GROUP,
				    SRYCD,
				    YUKOSTYMD
 		;
	};
};
