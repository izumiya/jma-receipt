tbl_teikiprt	{
        BTUNUM              char(2);
        DENPNUM             number(7);
        HOSPID              char(24);
        PTID                number(10);
        PTNUM               char(20);
        NAME                char(100);
};

primary	{
	BTUNUM,DENPNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_teikiprt_key_csr CURSOR FOR
		SELECT *
		FROM tbl_teikiprt
        WHERE   BTUNUM      =   :BTUNUM
	AND	DENPNUM     =   :DENPNUM
		;
	};
};

path	key2	{ 	
	DBSELECT	{
		DECLARE tbl_teikiprt_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_teikiprt
		ORDER   BY BTUNUM,NAME
		;
	};
};

