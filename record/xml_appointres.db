xml_appointres {
    object  object;
	mode	int;
	appointres {
        information_date					varchar(8);
        information_time					varchar(6);
        api_result							varchar(2);
        api_result_msg						varchar(100);
        reskey								varchar(100);
        appoint_date						varchar(8);
        appoint_time						varchar(4);
        appoint_id							varchar(5);
        appoint_dep_code					varchar(2);
        appoint_dep_name					varchar(80);
        appoint_dr_code						varchar(5);
        appoint_dr_name						varchar(80);
        appoint_memo						varchar(100);
	    patient_infomation {
        	patientid						varchar(20);
        	name							varchar(100);
        	kananame						varchar(100);
        	birthday						varchar(08);
        	sex								varchar(1);
        	address_info    {
           	 	home_zip_code				varchar(07);
            	home_addresses				varchar(100);
        	};
        	insurance_info  {
            	main_insurance_class		varchar(10);
