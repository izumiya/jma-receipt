fm_discharge_certificate {
  context                 int;
  object                  object;
  mode                    int;
  recordname              varchar(128);
  data{
    Form_ID               varchar(30);
    Custom_ID             varchar(30);
    Printer               varchar(20);
    Patient{
      ID                  varchar(20);
      Name                varchar(50);
      KanaName            varchar(50);
      BirthDate           varchar(10);
      Age                 varchar(3);
      Sex                 varchar(1);
    };
    Hospital{
      Name                varchar(120)[3];
      ZipCode             varchar(7);
      Address             varchar(200)[3];
      PhoneNumber         varchar(15);
    };
    Doctor{
      Code                varchar(5);
      Name                varchar(50);
      KanaName            varchar(50);
    };
    IssuedDate            varchar(10);
    Admission_Date        varchar(10);
    Discharge_Date        varchar(10);
    Hospital_Charge{
      Name                varchar(50);
      Days                varchar(5);
    }[3];
    Other_Days            varchar(5);
    Sum_Days              varchar(5);
    Sum_Start_Date        varchar(10);
    Sum_End_Date          varchar(10);
    General_Hospital_Charge{
      Days                varchar(5);
      Base_Date           varchar(10);
    };
    Disease_Name          varchar(76)[2];
    Special_Report        varchar(92)[8];
  };
};
