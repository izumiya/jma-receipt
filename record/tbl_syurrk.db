	KOH_FTN				integer;
	RSI_TOTAL			integer;
	JIHI_TOTAL  			integer;
	JIHI_TOTAL_TAX 			integer;
	JIHI_TAX  			integer;
	TOTAL_TGMONEY			integer;
	TOTAL_TGMONEY_TAX		integer;
        SKYMONEY_SKJ_JIHI_KEI           integer;
