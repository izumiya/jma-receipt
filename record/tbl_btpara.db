tbl_btpara {
	SHELLID 		char(8);
	RUN_OPID		char(16);
	DSP_RENNUM		number(3,0);
	RENNUM			number(4,0);
	JOBID			number(3,0);
	SCRIPTID		char(20);
	INFO_KBN		char(02);
	INFO_PARA		varchar(50);
	TERMID			varchar(16);
	OPID			varchar(16);
	CREYMD			char(8);
	UPYMD			char(8);
	UPHMS			char(6);
};

primary {
	SHELLID,RUN_OPID,DSP_RENNUM,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_btpara_key_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID    AND
					RUN_OPID    =   :RUN_OPID   AND
					DSP_RENNUM  =   :DSP_RENNUM AND
					RENNUM      =   :RENNUM
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_btpara_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID    AND
					SCRIPTID    =   :SCRIPTID   AND
					INFO_PARA   =   :INFO_PARA
		;
	};
};

path	key3		{ 	
	DBSELECT	{
		DECLARE tbl_btpara_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID  AND
					SCRIPTID    =   :SCRIPTID
					ORDER  BY  RENNUM
		;
	};
};

path	key4		{ 	
	DBSELECT	{
		DECLARE tbl_btpara_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID  AND
					SCRIPTID    =   :SCRIPTID AND
					RENNUM      =   :RENNUM
		;
	};
};

path	key5		{ 	
	DBSELECT	{
		DECLARE tbl_btpara_key5_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID  AND
					JOBID       =   :JOBID
					ORDER  BY  RENNUM
		;
	};
};

path	upd1	{ 	
	DBUPDATE	{
		UPDATE	tbl_btpara
				SET	JOBID       =   :JOBID
			WHERE   SHELLID     =   :SHELLID 
			AND     SCRIPTID    =   :SCRIPTID
		;
	};
};

path	del1	{
	DBDELETE	{
		DELETE
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID 
		;
	};
};

path	del2	{
	DBDELETE	{
		DELETE
		FROM tbl_btpara
			WHERE   SHELLID     =   :SHELLID  AND
					SCRIPTID    =   :SCRIPTID AND
					RENNUM      =   :RENNUM
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_btpara_all_csr CURSOR FOR
		SELECT *
		FROM tbl_btpara
 		;
	};
};
