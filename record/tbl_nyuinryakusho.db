tbl_nyuinryakusho	{
	SRYCD			char(9);
	YUKOSTYMD		char(8);
	YUKOEDYMD		char(8);
	IPROUKBN		char(1);
	RYAKUSHO		varchar(20);
	NAME			varchar(200);
	TERMID		varchar(16);
	OPID		varchar(16);
	CREYMD		char(8);
	UPYMD		char(8);
	UPHMS		char(6);
};

primary	{SRYCD,YUKOSTYMD,YUKOEDYMD;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_nyuinryakusho_key_csr CURSOR FOR
		SELECT *
		FROM tbl_nyuinryakusho
			WHERE
					SRYCD      =  :SRYCD
			AND		YUKOSTYMD     <=  :YUKOSTYMD
			AND		YUKOEDYMD     >=  :YUKOEDYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_nyuinryakusho_all_csr CURSOR FOR
		SELECT *
		FROM tbl_nyuinryakusho
      		;
	};
};
