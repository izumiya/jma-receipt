tbl_para {
	GYOUMUID			char(3);
	TERMID				char(64);
	FILEMEI 			char(20);
	EDANUM  			number(3,0);
	DATA_REC 			char(5000);
};
primary	{
	GYOUMUID,TERMID,FILEMEI,EDANUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_para_key_csr CURSOR FOR
		SELECT *
		FROM tbl_para
			WHERE   GYOUMUID   =   :GYOUMUID
			AND   TERMID       =   :TERMID
			AND   FILEMEI      LIKE  :FILEMEI
			AND   EDANUM       =   :EDANUM
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_para_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_para
			WHERE   GYOUMUID   =   :GYOUMUID
			AND   TERMID       =   :TERMID
			AND   FILEMEI      LIKE :FILEMEI
			ORDER   BY        EDANUM
		;
	};
};
path	del11 	{
	DBDELETE	{
		DELETE
		FROM tbl_para
			WHERE   GYOUMUID   =   :GYOUMUID
			AND   TERMID       =   :TERMID
			AND   FILEMEI      LIKE :FILEMEI
		;
	};
};
path	all 	{
	DBSELECT	{
		DECLARE tbl_para_all_csr CURSOR FOR
		SELECT *
		FROM tbl_para
 		;
	};
};
path	del2 	{
