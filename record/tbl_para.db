path	key3 	{
	DBSELECT	{
		DECLARE tbl_para_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_para
			WHERE   GYOUMUID   =   :GYOUMUID
			AND   TERMID       =   :TERMID
			ORDER   BY        FILEMEI,
