tbl_para {
	GYOUMUID			char(3);
	TERMID				char(32);
	FILEMEI 			char(20);
	EDANUM  			number(3,0);
	DATA_REC1 			char(1000);
	DATA_REC2 			char(1000);
	DATA_REC3 			char(1000);
	DATA_REC4 			char(1000);
	DATA_REC5 			char(1000);
};
primary	{
	GYOUMUID,TERMID,FILEMEI,EDANUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_para_key_csr CURSOR FOR
		SELECT *
		FROM tbl_para
			WHERE   GYOUMUID   =   :GYOUMUID
			AND   TERMID       =   :TERMID
			AND   FILEMEI      LIKE  :FILEMEI
			AND   EDANUM       =   :EDANUM
		;
	};
};

