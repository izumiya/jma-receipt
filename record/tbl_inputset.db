tbl_inputset {
	HOSPNUM				integer;
	SETCD   			char(6);
	YUKOSTYMD  			char(8);
	YUKOEDYMD	  		char(8);
	SETSEQ  			number(3);
	INPUTCD				char(9);
	SURYO1  			number(10,3);
	SURYO2				number(10,3);
	KAISU				number(3);
	COMENT				varchar(80);
	ATAI1				char(8);
	ATAI2				char(8);
	ATAI3				char(8);
	ATAI4				char(8);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD   			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPNUM,SETCD,YUKOSTYMD,YUKOEDYMD,SETSEQ;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_inputset_key_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
			AND   SETSEQ      =   :SETSEQ
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_inputset_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
			ORDER   BY        SETSEQ
		;
	};
};
path	del11	{
	DBDELETE	{
		DELETE
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   YUKOSTYMD   =   :YUKOSTYMD
			AND   YUKOEDYMD   =   :YUKOEDYMD
		;
	};
};
path	all 	{
	DBSELECT	{
		DECLARE tbl_inputset_all_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			ORDER   BY        SETCD,
                                          YUKOSTYMD,
                                          YUKOEDYMD,
                                          SETSEQ
 		;
	};
};

path	key3	{
	DBSELECT	{
		DECLARE tbl_inputset_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			ORDER   BY        SETCD,
                                          YUKOSTYMD,
			                  SETSEQ
		;
	};
};
path	key4	{
	DBSELECT	{
		DECLARE tbl_inputset_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
                        AND     SETCD     >=  :SETCD
                        AND     SETCD     <=  :UPHMS
			ORDER   BY        SETCD,
                                          YUKOSTYMD,
			                  SETSEQ
		;
	};
};
path	key5	{
	DBSELECT	{
		DECLARE tbl_inputset_key5_csr CURSOR FOR
		SELECT *
		FROM tbl_inputset
			WHERE   HOSPNUM    =   :HOSPNUM
			AND   SETCD       =   :SETCD
			AND   SETSEQ      =   1
			ORDER   BY        YUKOSTYMD,
			                  SETSEQ
		;
	};
};
