tbl_genericname {
	HOSPNUM				integer;
	YAKKAKJNCD			char(9);
	YUKOKETA			number(2,0);
	GENERICNAME			char(200);
	TANICD				char(3);
	TANINAME			char(24);
	KANZANCHI			number(10,5);
	TERMID   			char(16);
	OPID     			char(16);
	CREYMD   			char(8);
	UPYMD    			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPNUM,YAKKAKJNCD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_genericname_key_csr CURSOR FOR
		SELECT *
		FROM tbl_genericname
		WHERE 	HOSPNUM	= :HOSPNUM
		AND   YAKKAKJNCD    =   :YAKKAKJNCD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_genericname_all_csr CURSOR FOR
		SELECT *
		FROM tbl_genericname
		WHERE 	HOSPNUM	= :HOSPNUM
 		;
	};
};
