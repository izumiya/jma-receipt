tbl_etensu_1 {
	SRYCD				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	H_TANI1				number(2,0);
	H_GROUP1			varchar(7);
	H_TANI2				number(2,0);
	H_GROUP2			varchar(7);
	H_TANI3				number(2,0);
	H_GROUP3			varchar(7);
	R_DAY				number(1,0);
	R_MONTH				number(1,0);
	R_SAME				number(1,0);
	R_WEEK				number(1,0);
	N_GROUP				number(3,0);
	C_KAISU				number(1,0);
	CREYMD   			char(8);
};
primary	{
	SRYCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_1_key_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_1
		WHERE   SRYCD    =   :SRYCD
 		AND   YUKOSTYMD  <=  :YUKOSTYMD
		AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_1_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_1
		WHERE 	SRYCD      = :SRYCD
			ORDER   BY  YUKOSTYMD
		;
	};
};

path	key3 	{
	DBSELECT	{
		DECLARE tbl_etensu_1_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_1
		WHERE 	H_GROUP1      = :H_GROUP1
		    OR  H_GROUP2      = :H_GROUP1
		    OR  H_GROUP3      = :H_GROUP1
			ORDER   BY  YUKOSTYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_etensu_1_all_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_1
			ORDER   BY  SRYCD,
				    YUKOSTYMD
 		;
	};
};
