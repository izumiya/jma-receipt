INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@

prefix = @prefix@
exec_prefix= @exec_prefix@
recorddir = ${exec_prefix}/record

install_ignore := Makefile.in Makefile build-stamp

all:   
	touch build-stamp
clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f Makefile

install: all
	$(INSTALL) -d $(recorddir)
	$(INSTALL) *def *rec *db $(recorddir)
