tbl_psychotropic_master {
	CODE				varchar(7);
	YUKOSTYMD			varchar(8);
	YUKOEDYMD			varchar(8);
	CLASS				varchar(1);
	GROUPCODE			varchar(7);
	UPYMD				varchar(8);
#-----> 主キー更新用項目
	UPCODE				varchar(7),virtual;
	UPYUKOSTYMD			varchar(8),virtual;
	UPYUKOEDYMD			varchar(8),virtual;
};
primary	{
	CODE,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_psychotropic_master_key_csr CURSOR FOR
		SELECT *
		FROM master.tbl_psychotropic
			WHERE   CODE      =  :CODE
 			AND     YUKOSTYMD <= :YUKOSTYMD
			AND     YUKOEDYMD >= :YUKOEDYMD
		;
	};

	DBFETCH	{
		FETCH FROM	tbl_psychotropic_master_key_csr
			INTO
				:CODE,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:CLASS,
				:GROUPCODE,
				:UPYMD
		;
	};

	DBCLOSECURSOR	{
		CLOSE	tbl_psychotropic_master_key_csr
		;
	};

	DBINSERT	{
		INSERT INTO 	master.tbl_psychotropic
			VALUES(
				:CODE,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:CLASS,
				:GROUPCODE,
				:UPYMD
			)
		;
	};

	DBUPDATE		{
		UPDATE master.tbl_psychotropic
			SET
				CODE      = :CODE,
				YUKOSTYMD = :YUKOSTYMD,
				YUKOEDYMD = :YUKOEDYMD,
				CLASS     = :CLASS,
				GROUPCODE = :GROUPCODE,
				UPYMD     = :UPYMD 
			WHERE 	CODE      = :UPCODE
			AND	YUKOSTYMD = :UPYUKOSTYMD
			AND	YUKOEDYMD = :UPYUKOEDYMD
		;
	};

	DBDELETE	{
		DELETE FROM	master.tbl_psychotropic
			WHERE   CODE      = :CODE
 			AND     YUKOSTYMD = :YUKOSTYMD
			AND     YUKOEDYMD = :YUKOEDYMD
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_psychotropic_master_key2_csr CURSOR FOR
		SELECT *
		FROM master.tbl_psychotropic
			WHERE 	CODE = :CODE
			ORDER	BY	YUKOSTYMD
		;
	};

	DBFETCH	{
		FETCH FROM	tbl_psychotropic_master_key2_csr
			INTO
				:CODE,
				:YUKOSTYMD,
				:YUKOEDYMD,
				:CLASS,
				:GROUPCODE,
				:UPYMD
		;
	};

	DBCLOSECURSOR	{
		CLOSE	tbl_psychotropic_master_key2_csr
		;
	};
};
