tbl_csv_info	{
	HOSPNUM			number(2,0);
	TBL_KEY			char(8);
	SHELLID			varchar(20);
	SHORI_RENNUM		number(3);
	RENNUM  		number(3);
	SEQ_NO  		number(6);
	NYUGAIKBN		char(1);
	SRYYM			varchar(06);
	SKYYMD   		varchar(08);
	TEISYUTUSAKI		number(1);
	PTID			number(10);
	CSVDATA			varchar(2000);
	CREYMD 			varchar(8);
	CREHMS			varchar(6);
};

primary	{
	HOSPNUM,TBL_KEY,SRYYM,SHELLID,SHORI_RENNUM,RENNUM,SEQ_NO;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_csv_info_key_csr CURSOR FOR
		SELECT *
		FROM tbl_csv_info
		WHERE 	  HOSPNUM	=   :HOSPNUM
                    AND   TBL_KEY	=   :TBL_KEY
                    AND   SRYYM 	=   :SRYYM
                    AND   SHELLID	=   :SHELLID
                    AND   SHORI_RENNUM	=   :SHORI_RENNUM 
                    AND   RENNUM	=   :RENNUM
                    AND   SEQ_NO	=   :SEQ_NO
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_csv_info_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_csv_info
		WHERE 	  HOSPNUM	=   :HOSPNUM
                    AND   TBL_KEY	=   :TBL_KEY
                    AND   SRYYM 	=   :SRYYM
                    AND   SHELLID	=   :SHELLID
                    AND   SHORI_RENNUM	=   :SHORI_RENNUM 
                    AND   RENNUM	=   :RENNUM
                    ORDER BY SEQ_NO
		;
	};
};

path    del1    {
	DBDELETE        {
		DELETE
		FROM tbl_csv_info
		WHERE   HOSPNUM =   :HOSPNUM
		AND     TBL_KEY	=   :TBL_KEY
                ;
        };
};

