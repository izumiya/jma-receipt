rp_medication_note {
	context	int;
	object	object;
	mode	int;
    recordname	varchar(128);
	data {
		Report_ID				varchar(30);
		Custom_ID				varchar(30);
		Printer					varchar(20);
		Prescription_Date varchar(10);
		Invoice_Number varchar(7);
		Patient	{
			ID	 				varchar(20);
			Name 				varchar(50);
			KanaName  			varchar(50);
			BirthDate 			varchar(10);
			Sex					varchar(1);
		};
		Hospital	{
			Prefectures_Number  varchar(2);
			Name				varchar(120)[3];
			Address				varchar(200)[3];
			PhoneNumber			varchar(15);
		};
		Doctor	{
			Code				varchar(5);
			Name 				varchar(50);
		};
		Prescription	{
			Medical_Class		varchar(2);
			Amount				varchar(3);
			Total_Amount_Flg    varchar(1);
			Unit_Name			varchar(12);
			Medication	{
				Name				varchar(80);
				Amount				varchar(12);
				Unit_Name			varchar(12);
				Code				varchar(9);
			}[50];
		}[50];
		Memo						varchar(80)[10];
	};
};
