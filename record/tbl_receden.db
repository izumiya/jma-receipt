tbl_receden	{
HOSPID				char(24);
SRYYM				number(6);
NYUGAIKBN			char(1);
PTID				number(10);
RECEKA				char(2);
TEISYUTUSAKI		number(1,0);
RECESYUBETU			char(4);
HKNJANUM			char(8);
HOJOKBN_KEY			char(1);
TEKSTYMD			char(8);
RECKBN				char(1);
RENNUM				number(4);
CREYMD				char(8);
TOTALTEN			number(10);
RECEDATA			char(250);
};

primary	{
	HOSPID,SRYYM,NYUGAIKBN,PTID,RECEKA,TEISYUTUSAKI,RECESYUBETU,HKNJANUM,HOJOKBN_KEY,TEKSTYMD,RECKBN,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_receden_key_csr CURSOR FOR
		SELECT *
		FROM tbl_receden
		WHERE	HOSPID		=	:HOSPID
		  AND	SRYYM		=	:SRYYM
		  AND	NYUGAIKBN	=	:NYUGAIKBN
		  AND	PTID		=	:PTID
		  AND	RECEKA		=	:RECEKA
		  AND	TEISYUTUSAKI	=	:TEISYUTUSAKI
		  AND	RECESYUBETU	=	:RECESYUBETU
		  AND	HKNJANUM	=	:HKNJANUM
		  AND	HOJOKBN_KEY	=	:HOJOKBN_KEY
		  AND	TEKSTYMD	=	:TEKSTYMD
		  AND	RECKBN		=	:RECKBN
		  AND   RENNUM		=	:RENNUM
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_receden_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_receden
		WHERE	HOSPID		=	:HOSPID
		  AND	SRYYM		=	:SRYYM
		  AND	NYUGAIKBN	=	:NYUGAIKBN
		  AND	PTID		=	:PTID
		  AND	RECEKA		=	:RECEKA
		  AND	TEISYUTUSAKI	=	:TEISYUTUSAKI
		  AND	RECESYUBETU	=	:RECESYUBETU
		  AND	HKNJANUM	=	:HKNJANUM
		  AND	HOJOKBN_KEY	=	:HOJOKBN_KEY
		  AND	TEKSTYMD	=	:TEKSTYMD
		  AND	RECKBN		=	:RECKBN
          ORDER  BY  RENNUM
		;
	};
};


path	del1 	{
	DBDELETE	{
		DELETE
		FROM tbl_receden
                WHERE   HOSPID       =   :HOSPID
                  AND   SRYYM        =   :SRYYM
                  AND   NYUGAIKBN    =   :NYUGAIKBN
                  AND   TEISYUTUSAKI =   :TEISYUTUSAKI
		;
	};
};


