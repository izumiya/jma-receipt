path	key3	{
	DBSELECT	{
		DECLARE tbl_receden_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_receden
                WHERE (      HOSPID      =   :HOSPID     )
                  AND (      HKNNUM_S    <>  '060'       )
                  AND (      HKNNUM_S    <>  '067'       ) 
                  AND (      RECESYUBETU <>  '0000'      )
                  AND (   (( SRYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )
                       AND (( SKYYM       =  '000000' ) OR ( SKYYM = :SKYYM )))
                       OR (( SKYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )))
		;
	};
};

path	key4	{
