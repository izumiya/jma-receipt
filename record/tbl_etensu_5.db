tbl_etensu_5 {
	SRYCD				char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	TANICD				number(3,0);
	TANINAME			varchar(12);
	KAISU				number(3,0);
	TOKUREI				number(1,0);
	CHGYMD   			char(8);
};
primary	{
	SRYCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_etensu_5_key_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_5
		WHERE   SRYCD    =   :SRYCD
 		AND   YUKOSTYMD  <=  :YUKOSTYMD
		AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	key2 	{
	DBSELECT	{
		DECLARE tbl_etensu_5_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_5
		WHERE   SRYCD   =   :SRYCD
			ORDER   BY  YUKOSTYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_etensu_5_all_csr CURSOR FOR
		SELECT *
		FROM tbl_etensu_5
			ORDER   BY  SRYCD,
				    YUKOSTYMD
 		;
	};
};
