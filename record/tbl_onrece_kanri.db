path    upd1    {
        DBUPDATE        {
                UPDATE  tbl_onrece_kanri
                                SET
	UKEYMD		=	:UKEYMD,
	SKYYMD		=	:SKYYMD,
	RECV_FLG	=	:RECV_FLG,
	AMOUNT_FLG	=	:AMOUNT_FLG,
	SEND_FLG	=	:SEND_FLG,
	SEND_PATIENT_CNT	=	:SEND_PATIENT_CNT,
	SEND_TOTAL_CNT	=	:SEND_TOTAL_CNT,
	SEND_TOTAL_TENSU	=	:SEND_TOTAL_TENSU,
	SEND_TOTAL_LINECNT	=	:SEND_TOTAL_LINECNT
                WHERE	HOSPNUM		= :HOSPNUM
                    AND	TEISYUTUSAKI	= :TEISYUTUSAKI
                    AND	SRYYM		= :SRYYM
                    AND	UCNT		= :UCNT_V
                    AND	SCNT		= :SCNT_V
                    AND	RENNUM		= :RENNUM
