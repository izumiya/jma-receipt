tbl_pgkanri {
	HOSPNUM				integer;
	DEBIANVERSION                   varchar(24);
	SYSTEMVERSION                   varchar(10);
	PGNO                            char(3);
	RELEASEYMD                      char(8);
	PGNAME                          varchar(64);
	UPDATEFLG                       char(1);
	RESTARTFLG                      char(1);
	INSTALLDIR                      varchar(256);
	COMMENT                         varchar(400);
	TERMID  	        	varchar(32);
	OPID             		varchar(16);
	CREYMD  	        	char(8);
	UPYMD   	        	char(8);
	UPHMS		        	char(6);
};

primary	{
	HOSPNUM,DEBIANVERSION,SYSTEMVERSION,PGNO;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_pgkanri_key_csr CURSOR FOR
		SELECT *
		FROM tbl_pgkanri
		WHERE 	HOSPNUM	= :HOSPNUM
			AND   DEBIANVERSION  =  :DEBIANVERSION
			AND     SYSTEMVERSION  =  :SYSTEMVERSION
			AND     PGNO           =  :PGNO
			ORDER	BY  PGNO
                ;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_pgkanri_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_pgkanri
		WHERE 	HOSPNUM	= :HOSPNUM
			AND   UPDATEFLG  =  'F'
			ORDER	BY  PGNO
                ;
	};
};

path	all		{ 	
	DBSELECT	{
		DECLARE tbl_pgkanri_all_csr CURSOR FOR
		SELECT *
		FROM tbl_pgkanri
		WHERE 	HOSPNUM	= :HOSPNUM
			ORDER	BY  PGNO
		;
	};
};
