};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_ptnyuinrrk_key_csr CURSOR FOR
		SELECT *
		FROM tbl_ptnyuinrrk
			WHERE   HOSPID        =   :HOSPID 
			AND		PTID          =   :PTID
			AND		RRKNUM        =   :RRKNUM
			AND		RRKEDANUM     =   :RRKEDANUM
		;
	};
};

