tbl_generic_class {
	YAKKAKJNCD		varchar(12);
	YUKOSTYMD		varchar(8);
	YUKOEDYMD		varchar(8);
	KOUHATU	    		number(1,0);
	UPYMD    		varchar(8);
};
primary	{
	YAKKAKJNCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_generic_class_key_csr CURSOR FOR
		SELECT *
		FROM tbl_generic_class
			WHERE YAKKAKJNCD =   :YAKKAKJNCD
 			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};
