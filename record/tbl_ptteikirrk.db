tbl_ptteikirrk {
       SRYYM           char(6);
       PTID            number(10);
       SAKKBN          char(1);
       HKNCOMBINUM     number(4);
       SKYSTYMD        char(8);
       SKYEDYMD        char(8);
       SKYTEN          number(9);
       SKYMONEY        number(9);
       TERMID          varchar(16);
       OPID            varchar(16);
       CREYMD          varchar(8);
       UPYMD           varchar(8);
       UPHMS           varchar(6);
};
primary	{
	SRYYM,PTID,SAKKBN,HKNCOMBINUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_ptteikirrk_key_csr CURSOR FOR
		SELECT *
		FROM tbl_ptteikirrk
        WHERE  SRYYM   =   :SRYYM
        AND    PTID    =   :PTID
        AND    SAKKBN  =   :SAKKBN
        AND    HKNCOMBINUM = :HKNCOMBINUM
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_ptteikirrk_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_ptteikirrk
        WHERE  SRYYM   =   :SRYYM
        AND    PTID    =   :PTID
		;
	};
};

path	key3		{ 	
	DBSELECT	{
		DECLARE tbl_ptteikirrk_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_ptteikirrk
        WHERE  PTID    =   :PTID
		ORDER  BY  SKYEDYMD DESC
		;
	};
};

path	key4		{ 	
	DBSELECT	{
		DECLARE tbl_ptteikirrk_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_ptteikirrk
        WHERE  PTID        = :PTID
		AND    SKYSTYMD   >= :SKYSTYMD
		AND    SKYEDYMD   <= :SKYEDYMD
		;
	};
};

path	all		{ 	
	DBSELECT	{
		DECLARE tbl_ptteikirrk_all_csr CURSOR FOR
		SELECT *
		FROM tbl_ptteikirrk
		;
	};
};

path	del1		{ 	
	DBDELETE	{
		DELETE
		FROM tbl_ptteikirrk
        WHERE  PTID        = :PTID
		;
	};
};

path	del2		{ 	
	DBDELETE	{
		DELETE
		FROM tbl_ptteikirrk
        WHERE  PTID        = :PTID
		AND    SKYSTYMD   >= :SKYSTYMD
		AND    SKYEDYMD   <= :SKYEDYMD
		;
	};
};

