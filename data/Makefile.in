COBOL = cobc
INSTALL = @INSTALL@
MKDIR = mkdir -p
CP = cp

prefix = @prefix@
exec_prefix= @exec_prefix@
orcadir = ${exec_prefix}/orca
datadir = ${orcadir}/data
install_ignore := Makefile.in Makefile build-stamp

all:   
	touch build-stamp
clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f Makefile

install: all
	$(INSTALL) -d $(datadir)
	$(CP) -R * $(datadir)/; \
	for i in $(install_ignore) ;do rm -f $(datadir)/$$i; done
