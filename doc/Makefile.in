INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@

prefix = @prefix@
exec_prefix= @exec_prefix@
docdir = ${prefix}/doc

EXTRA_DIST = \
        copyright \
        license.html \
        orcarc \
        version \
        examples/.orcapwd 

all:   
	touch build-stamp
clean: 
	-rm -f build-stamp

distclean: 
	-rm -f build-stamp
	-rm -f Makefile

install: all
	$(INSTALL) -d $(docdir)
	$(INSTALL_DATA) $(EXTRA_DIST) $(docdir)
